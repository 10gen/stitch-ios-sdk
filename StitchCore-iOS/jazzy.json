{
    "xcodebuild_arguments": ["-workspace", "../Stitch.xcworkspace", "-scheme", "StitchCore-iOS"],
    "author": "MongoDB",
    "author_url": "https://github.com/mongodb/stitch-ios-sdk",
    "module": "StitchCore_iOS",
    "theme": "fullwidth",

    "documentation":"DocsSources/Pages/*.md",
    "abstract":"DocsSources/Abstracts/*.md",

    "exclude": "/*/Internal*",

    "custom_categories": [{
        "name": "Getting Started",
        "children": [
            "Note on StitchCore Module",
            "Notes on Multithreading"
        ]
    }, {
        "name": "Interacting with Stitch",
        "children": [
            "Stitch",
            "StitchAppClient",
            "StitchAuth",
            "StitchUser"
        ]
    }, {
        "name": "Delegates",
        "children": [
            "StitchAuthDelegate"
        ]
    }, {
        "name": "Errors",
        "children": [
            "StitchInitializationError"
        ]
    }, {
        "name": "Authentication Providers",
        "children": [
            "AnonymousAuthProvider",
            "AnonymousAuthProviderClient",
            "CustomAuthProvider",
            "CustomAuthProviderClient",
            "FacebookAuthProvider",
            "FacebookAuthProviderClient",
            "GoogleAuthProvider",
            "GoogleAuthProviderClient",
            "ServerAPIKeyAuthProvider",
            "ServerAPIKeyAuthProviderClient",
            "UserAPIKeyAuthProvider",
            "UserAPIKeyAuthProviderClient",
            "UserPasswordAuthProvider",
            "UserPasswordAuthProviderClient"
        ]
    }]
}
