{
    "xcodebuild_arguments": ["-project", "StitchCore.xcodeproj", "-target", "StitchCore"],
    "author": "MongoDB",
    "author_url": "https://github.com/mongodb/stitch-ios-sdk",
    "module": "StitchCore",
    "theme": "fullwidth",

    "documentation":"DocsSources/Pages/*.md",
    "abstract":"DocsSources/Abstracts/*.md",

    "exclude": "/*/Internal*",

    "custom_categories": [{
        "name": "Overview",
        "children": [
            "Warning"
        ]
    }, {
        "name": "Client Configuration",
        "children": [
            "StitchAppClientConfigurationBuilder",
            "StitchAppClientConfiguration",
            "StitchClientConfiguration"
        ]

    }, {
        "name": "Users",
        "children": [
            "StitchUserProfile",
            "StitchUserIdentity"
        ]
    }, {
        "name": "Authentication",
        "children": [
            "StitchCredential",
            "ProviderCapabilities"
        ]
    }, {
        "name": "Errors",
        "children": [
            "StitchError",
            "StitchErrorCode",
            "StitchErrorCodable",
            "StitchClientErrors",
            "StitchServiceException"
        ]
    }]
}
